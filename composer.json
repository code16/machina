{
    "name": "code16/machina",
    "description": "Machine to machine authentication for Laravel",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0|^9.0",
        "php-open-source-saver/jwt-auth": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.5",
        "orchestra/testbench": "6.*|7.*"
    },
    "autoload": {
       "psr-4": {
            "Code16\\Machina\\": "src/Machina"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code16\\Machina\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Code16\\Machina\\MachinaServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
