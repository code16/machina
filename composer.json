{
    "name": "code16/machina",
    "description": "Machine to machine authentication for Laravel",
    "keywords": ["auth", "laravel", "api", "package"],
    "homepage": "https://github.com/code16/machina",
    "authors": [
        {
            "name": "RÃ©mi Collin",
            "email": "remi@code16.fr"
        }
    ],
    "repositories": [
        {
            "type" : "vcs",
            "url": "https://github.com/code16/responder.git"
        }
    ],
    "require": {
        "illuminate/support": "5.6.*",
        "tymon/jwt-auth": "^0.5.12",
        "code16/responder": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "7.0.*",
        "orchestra/testbench": "3.6.*"
    },
    "autoload": {
       "psr-4": {
            "Code16\\Machina\\": "src/Machina"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Code16\\Machina\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Code16\\Machina\\ServiceProvider"
            ]
        }
    },
